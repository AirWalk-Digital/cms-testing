    "concurrently": "^5.0.0",
    "gray-matter": "^4.0.2",
    "moment": "^2.24.0",
    "next": "^9.0.2",
    "node-sass": "^4.12.0",
    "raw-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.1.0",
    "react-tinacms": "^0.6.0",
    "tinacms": "^0.6.0",
    "webpack": "^4.37.0"
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  },
  "keywords": [
    "next",
    "forestry"
