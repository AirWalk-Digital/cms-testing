{
  "name": "foresry-starter-blog-next",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Forestry & Next",
  "license": "MIT",
  "version": "0.1.0",
  "author": "@kendallstraut",
  "scripts": {
    "dev": "next src",
    "develop": "concurrently \"next src\" \"tina-git-server 3001\"",
